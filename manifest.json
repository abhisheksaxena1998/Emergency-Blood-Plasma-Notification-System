{ "manifest_version": 2,
        "background": {
            "scripts": ["popup.js"],
            "persistent": true
        },
    
 "name":"Serum Ledger",
 "description":"Emergency notification system",
 "version":"4.0.0",
 "icons":{"128":"unnamed1.png"},
 "options_page": "options.html",
 "browser_action": {
     "default_icon": "unnamed1.png",
     "default_popup": "popup.html"
 },
 "web_accessible_resources": [
    "bg1.jpg"
  ],
 "permissions":["geolocation","https://detecturl.herokuapp.com/","storage","tabs","activeTab","<all_urls>","webRequest","webRequestBlocking","https://*/","notifications" ]
}